// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model PropellerData {
  id String @default(cuid()) @id
  propeller1 Int
  propeller2 Int
  propeller3 Int
  propeller4 Int
  propeller5 Int
  date DateTime @default(now())
}